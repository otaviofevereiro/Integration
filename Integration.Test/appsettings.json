{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "Amqp": {
    "Type": "RabbitMQ",
    "Url": "http://localhost:5672/",
    "VirtualHost": "/",
    "UserName": "guest",
    "Password": "guest",
    "Queues": [
      {
        "Name": "people",
        "Durable": true,
        "Exclusive": false,
        "AutoDelete": false,
        "Exchange": "people-exchange",
        "ExchangeType": "direct",
        "RouteKey": "PersonEvent"
      }
    ]
  },

  "Amqp2": {
    "Type": "RabbitMQ",
    "Url": "http://localhost:5672/",
    "VirtualHost": "/",
    "UserName": "guest",
    "Password": "guest",
    "Queues": [
      {
        "Name": "address",
        "Durable": true,
        "Exclusive": false,
        "AutoDelete": false,
        "Exchange": "address",
        "ExchangeType": "direct",
        "RouteKey": "AddressEvent"
      }
    ]
  }
}
